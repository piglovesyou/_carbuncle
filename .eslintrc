{
  "extends": "eslint:recommended",
  "env": {
    "node": true,
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "objectLiteralShorthandMethods": true,
      "objectLiteralShorthandProperties": true,
      "destructuring": true,
      "arrowFunctions": true,
      "jsx": true
    }
  },
  "rules": {
  },
  "globals": {
    "goog": true
  }
}

